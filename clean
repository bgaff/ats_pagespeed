rm -fr configure
rm -fr depcomp
rm -fr install-sh
rm -fr Makefile
rm -fr Makefile.in
rm -fr missing
rm -fr config.guess
rm -fr config.sub
rm -fr libtool
rm -fr ltmain.sh
rm -fr m4
rm -rf src/*.la
